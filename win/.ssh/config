# cloudflared 访问远程服务器
Host cloud
  ServerAliveInterval 60	
  HostName ssh.202309.xyz
  User root
  ProxyCommand C:\PROGRA~2\cloudflared\cloudflared.exe access ssh --hostname %h
  
# OpenWRT 主路由
Host openwrt
  HostName 192.168.31.1
  User root

# AX6S 旁路由
Host ax6s
  HostName 192.168.31.2
  User root

# 小米手机
