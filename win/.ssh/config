# cloudflared 访问远程服务器
Host cloud
  ServerAliveInterval 60	
  HostName ssh.202309.xyz
  User huai
  ProxyCommand "/c/Program Files (x86)/cloudflared/cloudflared.exe" access ssh --hostname %h  

